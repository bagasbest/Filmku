<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.filmku.filmku">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Filmku">
        <activity android:name=".favorite.FavoriteActivity" />
        <activity android:name=".movie.MovieActivity" />
        <activity android:name=".movie.MovieEditActivity" />
        <activity android:name=".movie.MovieDetailActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".movie.MovieAddActivity" />
        <activity android:name=".HomepageActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>